\n    localStorage.removeItem('id_token');\n    window.location.reload();\n  }\n  if (Auth.loggedIn()) {\n    return (\n      <button onClick={logOut}>Log Out</button>\n    )\n  } else\n  \n  return (\n    \n    <div className=\"Login\">\n      <div className=\"title\">\n        <button\n          className={`${state ? \"black\" : \"red\"}`}\n          onClick={() => setState(false)}\n        >\n          Login\n        </button>\n        &nbsp;/&nbsp;\n        <button\n          className={`${state ? \"red\" : \"black\"}`}\n          onClick={() => setState(true)}\n        >\n          SignUp\n        </button>\n      </div>\n      {state ? (\n        <form className=\"\" onSubmit={handleSignupSubmit}>\n          <div>\n          <input\n            type=\"text\"\n            name=\"username\"\n            placeholder=\"Username\"\n            value={signUpFormState.username}\n            onChange={handleSignUpChange}\n          />\n          </div>\n          <input\n            type=\"email\"\n            name=\"email\"\n            placeholder=\"Email\"\n            value={signUpFormState.email}\n            onChange={handleSignUpChange}\n          />\n          <div>\n          <input\n            type=\"password\"\n            name=\"password\"\n            placeholder=\"Password\"\n            value={signUpFormState.password}\n            onChange={handleSignUpChange}\n          />\n          </div>\n          {/* <input type='password' placeholder='Confirm Password' /> */}\n          <div>\n            <button className=\"btn btn-primary\" type=\"submit\">\n              Sign Up\n            </button>\n          </div>\n        </form>\n      ) : (\n        <form className=\"body\" onSubmit={handleLoginSubmit}>\n          <div>\n          <input type=\"email\" name=\"email\" value={loginFormState.email} onChange={handleLoginChange} placeholder=\"Email\" />\n          </div>\n          <div>\n          <input type=\"password\" name=\"password\" value={loginFo